package com.completeperspective.media.model{	import com.completeperspective.media.events.*;	import com.completeperspective.vo.MediaVO;	import com.completeperspective.xml.*;		import flash.events.*;		/**	 * Playlists are used to create lists or stations of media to be played for a single player instance.	 * 	 * @author adam	 */	public class Playlist extends RSSReader	{		public static const VERSION : String = "1.0.0";				public var items : Array;		public var media : Namespace = new Namespace( "http://search.yahoo.com/mrss/" );				public function Playlist() : void		{			super();						items = new Array();		}				/**		 * Parses MRSS Playlist		 */		override public function parseFeed( rss : XML ) : void		{			super.parseFeed( rss );						var feedItems : XMLList = rss.channel..item;						for( var i : int = 0; i < feedItems.length(); i++ )			{				var vo : MediaVO = new MediaVO();								// RSS Nodes				vo.title = feedItems[i].title;				vo.description = feedItems[i].description;				vo.link = feedItems[i].link;								// MRSS Content Node Attributes				vo.bitrate = feedItems[i]..media::content.@bitrate;				vo.channels = feedItems[i]..media::content.@channels;				vo.duration = feedItems[i]..media::content.@duration;				vo.expression = feedItems[i]..media::content.@expression;				vo.fileSize = feedItems[i]..media::content.@fileSize;				vo.framerate = feedItems[i]..media::content.@framerate;				vo.height = feedItems[i]..media::content.@height;				vo.isDefault = Boolean( feedItems[i]..media::content.@isDefault );				vo.lang = feedItems[i]..media::content.@lang;				vo.medium = feedItems[i]..media::content.@media;				vo.samplingrate = feedItems[i]..media::content.@samplingrate;				vo.type = feedItems[i]..media::content.@type;				vo.url = feedItems[i]..media::content.@url;				vo.width = feedItems[i]..media::content.@width								items.push( vo );				}									dispatchEvent( new PlaylistEvent( PlaylistEvent.PLAYLIST_READY, this ) );					}				override public function toString():String		{			return "[PlaylistModel v" + VERSION + "]"; 		}	}}